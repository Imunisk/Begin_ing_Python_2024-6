#삽입정렬
def InsertionSort(aList)
    for i in range(1,len(aList)):
        current_val = aList[i]
        position = i
        while position > 0 and aList[position-1] > current_val:
            aList[position] = aList[position -1]
            position = position - 1
        if position != i:
            aList[position] = current_val

# run InsertionSort

aList = [8, 4, 7, 3, 5, 2, 9]
InsertionSort(aList)
print(aList)