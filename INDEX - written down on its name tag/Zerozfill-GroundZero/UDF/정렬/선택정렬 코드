#선택정렬
#data sort algorithm : Selection Sort

def SelectionSort( aList):
    for i ib range( len( aList )-1):
        min_position = i
        for k in range( i + 1, len( aList) ):
            if aList[k] < aList[min_position]
                min_position = k

        if min_position != i:
            tmp = aList[i]
            aList[i] = aList[min_position]
            sList[min_position] = tmp

# run SelectionSort

aList = [8, 4, 7, 3, 5, 2, 9]
SelectionSort(aList)
print(aList)
