# 딕셔너리 선언
dic = {
    "name" : "7D 건조 망고"
    "type" : "당절임"
    "ingredient" : ["망고", "설탕", "메타중아황산나트륨", "치자황색소"
    "origin" : "필리핀"
}


# 출력
print("name: ", dic["name"])
print("type: ", dic["type"])
print("ingredient: ", dic["ingredient"])
print("origin: ", dic["origin"])
print()

# 값을 변경
dic["name"] = "8D 건조 망고"
print("name: ", dic["name"])
